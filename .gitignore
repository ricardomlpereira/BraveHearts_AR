# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/AR_MemoryGame/[Ll]ibrary/
/AR_MemoryGame/[Tt]emp/
/AR_MemoryGame/[Oo]bj/
/AR_MemoryGame/[Bb]uild/
/AR_MemoryGame/[Bb]uilds/
/AR_MemoryGame/[Ll]ogs/
/AR_MemoryGame/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/AR_MemoryGame/[Mm]emoryCaptures/

# Recordings can get excessive in size
/AR_MemoryGame/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /AR_MemoryGame/[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/AR_MemoryGame/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/AR_MemoryGame/.vs/

# Gradle cache directory
/AR_MemoryGame/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/AR_MemoryGame/*.csproj
/AR_MemoryGame/*.unityproj
/AR_MemoryGame/*.sln
/AR_MemoryGame/*.suo
/AR_MemoryGame/*.tmp
/AR_MemoryGame/*.user
/AR_MemoryGame/*.userprefs
/AR_MemoryGame/*.pidb
/AR_MemoryGame/*.booproj
/AR_MemoryGame/*.svd
/AR_MemoryGame/*.pdb
/AR_MemoryGame/*.mdb
/AR_MemoryGame/*.opendb
/AR_MemoryGame/*.VC.db

# Unity3D generated meta files
/AR_MemoryGame/*.pidb.meta
/AR_MemoryGame/*.pdb.meta
/AR_MemoryGame/*.mdb.meta

# Unity3D generated file on crash reports
/AR_MemoryGame/sysinfo.txt

# Builds
/AR_MemoryGame/*.apk
/AR_MemoryGame/*.aab
/AR_MemoryGame/*.unitypackage
/AR_MemoryGame/*.app

# Crashlytics generated file
/AR_MemoryGame/crashlytics-build.properties

# Packed Addressables
/AR_MemoryGame/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/AR_MemoryGame/[Aa]ssets/[Ss]treamingAssets/aa.meta
/AR_MemoryGame/[Aa]ssets/[Ss]treamingAssets/aa/*

# MacOS files
**/.DS_Store

# Hidden word file
~$*
